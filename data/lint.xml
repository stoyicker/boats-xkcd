<lint>
  <issue id="GoogleAppIndexingWarning" severity="ignore"/>
  <issue id="InvalidPackage">
    <ignore regexp="retrofit"/>
    <ignore regexp="okio"/>
    <ignore regexp="dagger"/>
  </issue>
  <issue id="UnusedResources">
    <!-- Eaten by PSync -->
    <ignore regexp="prefs_dump.xml"/>
    <!-- Eaten by PSync -->
    <ignore regexp="prefs_settings.xml"/>
  </issue>
  <issue id="CommitPrefEdits">
    <!-- Intended PSync behavior -->
    <ignore path="*/P.java"/>
  </issue>
  <issue id="IconLocation">
    <ignore path="*/ic_launcher.png"/>
  </issue>
  <issue id="RtlHardcoded" severity="ignore"/>
    <issue id="InconsistentLayout">
    <!-- Looks like a bug -->
    <ignore regexp="appcompat-v7"/>
  </issue>
</lint>
